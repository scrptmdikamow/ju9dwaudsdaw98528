BannedPlayers = {"Squrds"}
local Cooldown = 5 -- Secounds between check on players

function CheckBan()
for i,j in pairs(game.Players:GetPlayers()) do
for x,y in pairs(BannedPlayers) do
if string.find(string.lower(j.Name),string.lower(y)) then
j:remove()
end end end end

CheckBan()
wait(Cooldown)
CheckBan()
